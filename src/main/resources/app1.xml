<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="employee" class="com.hocviencntt.springresot.springresort.Day6.Employee" p:name="Viet"
          p:swimmingPool-ref="swimming"></bean>
    <bean id="swimming" class="com.hocviencntt.springresot.springresort.Day6.SwimmingPool" p:name="Swimming1" p:timeOn="10:00AM"></bean>
</beans>